{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": {
            "Service": "lambda.amazonaws.com"
        },
        "Action": "SQS:SendMessage",
        "Resource": "arn:aws:sqs:us-east-1:000000000000:SendEmailsDlq",
        "Condition": {
          "StringEquals": {
            "aws:SourceArn": "arn:aws:lambda:us-east-1:000000000000:function:sendEmails"
          }
        }
      }
    ]
  }
  